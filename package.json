{
  "name": "@specialblend/apollo-blackbox",
  "version": "1.0.2",
  "description": "React Apollo Providers with loading, error and data states",
  "author": "specialblend",
  "license": "MIT",
  "repository": "https://github.com/specialblend/apollo-blackbox",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "@apollo/client": "^3.0.0-beta.43",
    "apollo-link": "^1.2.13",
    "graphql-tools": "^4.0.7"
  },
  "devDependencies": {
    "@specialblend/eslint-config": "^0.5.4",
    "@apollo/client": "^3.0.0-beta.43",
    "apollo-link": "^1.2.13",
    "apollo-link-schema": "^1.2.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^2.2.0",
    "graphql-tools": "^4.0.7",
    "microbundle-crl": "^0.13.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
